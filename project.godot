}

[application]

config/name="FNF Godot"
run/main_scene="res://Scenes/SceneSwitcher.tscn"
config/icon="res://icon.png"

[autoload]

Conductor="*res://Scripts/Singletons/Conductor.gd"
Debug="*res://Scripts/Singletons/Debug.gd"
Main="*res://Scripts/Singletons/Main.gd"
Settings="*res://Scripts/Singletons/Settings.gd"

[display]

window/size/width=1280
window/size/height=720
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Sprite_Font_Generator/plugin.cfg" )

[global]

fps=false

[importer_defaults]

ogg_vorbis={
"loop": false,
"loop_offset": 0
}

